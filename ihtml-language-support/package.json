{
  "name": "ihtml-language-support",
  "displayName": "IHTML Language Support",
  "publisher": "ua2",
  "repository": "https://github.com/Burduk2/ihtml",
  "description": "Syntax highlighting and other features for IHTML language",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ihtml",
      "aliases": ["IHTML", "ihtml"],
      "extensions": [".ihtml"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ihtml",
      "scopeName": "source.ihtml",
      "path": "./syntaxes/ihtml.tmLanguage.json"
    }]
  }
}
